<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ica.han.oose.project.overhoorapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ica.han.oose.project.overhoorapp.login.LoginActivity"
            android:label="LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ica.han.oose.project.overhoorapp.overhoren.QuizActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_Quiz" >
        </activity>
        <activity
            android:name="ica.han.oose.project.overhoorapp.account.Register"
            android:parentActivityName="ica.han.oose.project.overhoorapp.login.LoginActivity" />
        <activity
            android:name="ica.han.oose.project.overhoorapp.overhoren.RehearsalActivity"
            android:configChanges="orientation|screenSize"
            android:label="Overhoringen" >
        </activity>
        <activity
            android:name="ica.han.oose.project.overhoorapp.knowledge.SummaryActivity"
            android:configChanges="orientation|screenSize"
            android:label="TopicCreateView" >
        </activity>
        <activity android:name="ica.han.oose.project.overhoorapp.statistics.StatisticsActivity" />
        <activity
            android:name="ica.han.oose.project.overhoorapp.knowledge.TopicActivity"
            android:label="topic"
            android:parentActivityName="ica.han.oose.project.overhoorapp.knowledge.SummaryActivity" >
        </activity>
        <activity android:name="ica.han.oose.project.overhoorapp.question.QuestionActivity" />
        <activity
            android:name="android.support.v7.widget.TestActivity"
            android:label="RecyclerViewTestActivity" />
    </application>

</manifest>